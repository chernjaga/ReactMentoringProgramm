{
    "presets": [
        "@babel/preset-typescript",
        [
            "@babel/preset-react",
            {
                "throwIfNamespace": true,
                "runtime": "classic"
            }
        ],
        [
            "@babel/preset-env",
            {
                "targets": "> 0.25%, not dead"
            }
        ]

    ],
    "plugins": [
        "@babel/plugin-transform-typescript",
        [
            "@babel/plugin-proposal-class-properties", {
                "loose": true
            }
        ],
        [
            "babel-plugin-styled-components", {
                "minify": false,
                "fileName": false,
                "transpileTemplateLiterals": false,
                "pure": true
            }
        ]
    ]
}